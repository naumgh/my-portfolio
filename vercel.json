{
    "version": 2,
    "builds": [
      { "src": "src/app.html", "use": "@sveltejs/adapter-static" },
      { "src": "api/*.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "api/send_email.py" },
      { "src": "/(.*)", "dest": "/src/routes/+page.svelte" }
    ]
  }